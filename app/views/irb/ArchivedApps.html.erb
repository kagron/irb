<table class="table">                         
  <tr>                          
    <th>id</th>
    <th>First Name</th>
	<th>Last Name</th>
    <th>Phone</th>
	<th>Email</th>
    <th>Type</th>
	<th>Date Submitted</th>
    <th>Questions File</th>
	<th>Consent File</th>
    <th>Advisor Signature</th>
  </tr>
<% @data.each do |data| %> 
  <tr>
    <td><%= data.id %></td>
    <td><%= data.fName %></td>
	<td><%= data.phone %></td>
    <td><%= data.email %></td>
	<td><%= data.typeOfApplication %></td>
    <td><%= data.created_at %></td>
	<td><%= data.questions_file %></td>
    <td><%= data.consent_file %></td>
	<td><%= data.advisor_sig %></td>
  </tr>
<% end %>                      
</table> 
