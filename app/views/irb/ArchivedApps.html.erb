<html>
<div class="container">
  <div class="form-group">
    <%= form_tag(irb_ArchivedApps_path, :method => "get", id: "search-form") do %>
    <%= text_field_tag :search, params[:search], placeholder: "Search Posts", class: 'form_control' %>
    <%= submit_tag "Search", class: 'btn btn-default' %>
  </div>
<% end %></br>
<table class="table">
  <tr>
    <th>id</th>
    <th>First Name</th>
	<th>Last Name</th>
    <th>Phone</th>
	<th>Email</th>
    <th>Type</th>
	<th>Date Submitted</th>
    <th>Advisor Signature</th>
  </tr>
<% @data.each do |data| %>
  <tr>
    <td><%= data.id %></td>
    <td><%= data.fName %></td>
	<td><%= data.lName %></td>
	<td><%= data.phone %></td>
    <td><%= data.email %></td>
	<td><%= data.typeOfApplication %></td>
    <td><%= data.created_at %></td>
	<td><%= data.advisor_sig %></td>
  </tr>
<% end %>
</table>
</div>
</html>
